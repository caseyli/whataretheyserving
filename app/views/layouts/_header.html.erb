<% flash.each do |key, value| %>
	<div class="flash <%= key %>"><%= value %></div>
<% end %>

<div class="nav">
	<%= link_to "HOME", "/home" %>
	<%= link_to "ABOUT", "/about" %>
	<% if user_signed_in? %>
		<%= link_to "SIGN OUT", destroy_user_session_path, :method => :delete %>
	<% else %>
		<%= link_to "SIGN IN", "/signin" %>
	<% end %>
</div>